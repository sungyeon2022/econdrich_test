<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ecoandrichtest.employee.mapper.EmployeeMapper">
    <select id="findEmployeeById" parameterType="java.util.Map" resultType="com.example.ecoandrichtest.employee.vo.EmployeesQueryVO">
        select *
        from employees em
        where em.employee_id = #{empId}
    </select>
</mapper>